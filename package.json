  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "dev": "vite",
    "format:lint": "yarn format && yarn lint",
    "prettier": "prettier  \"**/*.+(json|yml|css|md|mdx|ts|tsx)\" --check",
    "prepare": "husky",
    "prettier:fix": "prettier --write \"**/*.+(json|yml|css|md|mdx|ts|tsx)\"",
    "lint-staged": "lint-staged",
    "lint:fix": "eslint --fix \"src/**/*.{ts,tsx,js,jsx,cjs,mjs}\"",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx,cjs,mjs}\"",
    "preview": "vite preview",
    "test": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "npx chromatic --project-token=chpt_e5fcb3682ee4f55"
  },
  "dependencies": {
